<!-- Agregar JavaScript al final de la página -->
<script>
  // Obtener el formulario y agregar un evento submit
  const formulario = document.querySelector('#formulario-contacto');
  formulario.addEventListener('submit', async (event) => {
    // Cancelar el comportamiento predeterminado del formulario
    event.preventDefault();

    // Enviar la solicitud del formulario con fetch
    const response = await fetch('/send-email', {
      method: 'POST',
      body: new FormData(formulario)
    });

    // Verificar si la solicitud fue exitosa
    if (response.ok) {
      // Mostrar el toast de éxito y redirigir al usuario a la página de confirmación
      Swal.fire({
        title: 'Mensaje enviado',
        text: 'Tu mensaje ha sido enviado correctamente',
        icon: 'success',
        confirmButtonText: 'OK'
      }).then(() => {
        window.location.href = '/contacto';
      });
    } else {
      // Mostrar un toast de error si la solicitud falló
      Swal.fire({
        title: 'Error',
        text: 'Ocurrió un error al enviar el mensaje. Por favor, inténtalo de nuevo más tarde.',
        icon: 'error',
        confirmButtonText: 'OK'
      });
    }
  });
</script>

